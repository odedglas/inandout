        "nodemailer": "^4.6.7",
        "npm": "^6.3.0",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "promise": "8.0.1",
        "prop-types": "^15.6.1",
        "raf": "3.4.0",
